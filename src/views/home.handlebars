<h1>Elementos agregados</h1>
<div id="products">

  {{#each products}}
    <div class="producto">
      <li>
        {{this.title}}
      </li>
      <p>Descripcion: {{this.description}}</p>
      <p>Categoria: {{this.category}}</p>
      <p>Precio: {{this.price}}</p>
      <p>Id: {{this.productId}}</p>
    </div>
  {{/each}}

</div>
<p>
  <button>
    <a href="/">
      Inicio</a></button>
</p>